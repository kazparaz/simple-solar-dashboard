{
  "scripts": {
    "start": "snowpack dev",
    "build": "yarn lint && snowpack build",
    "test": "echo \"There are no test runner.\" && exit 1",
    "lint": "tsc --noEmit && eslint 'src/**/*.{ts,js,tsx,jsx}' --quiet --fix && prettier --write 'src/**/*.{ts,js,tsx,jsx,json}'"
  },
  "dependencies": {
    "solid-app-router": "0.0.10",
    "solid-js": "^0.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@snowpack/plugin-babel": "^2.1.4",
    "@types/snowpack-env": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-preset-solid": "0.21.3",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-functional": "^3.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2",
    "snowpack": "^2.16.0",
    "typescript": "^4.0.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "yarn lint"
    ]
  }
}
